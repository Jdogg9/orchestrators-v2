{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "datasource": "Prometheus",
      "description": "Tracks the ratio of used vs. max tokens across requests.",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "scaleDistribution": {
              "type": "linear"
            }
          },
          "unit": "none"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "heatmap": {},
      "id": 1,
      "options": {
        "calculate": false,
        "cellGap": 2,
        "color": {
          "exponent": 0.5,
          "fill": "rgba(70, 118, 145, 0.9)",
          "mode": "scheme",
          "scheme": "RdYlGn",
          "steps": 64
        },
        "exemplars": {
          "color": "rgba(255,0,255,0.7)"
        },
        "filterValues": {
          "le": 1e-9
        },
        "legend": {
          "show": true
        },
        "rowsFrame": {
          "layout": "auto"
        },
        "tooltip": {
          "show": true,
          "yHistogram": true
        }
      },
      "targets": [
        {
          "exemplar": true,
          "expr": "sum(rate(orch_token_utilization_ratio_bucket[5m])) by (le)",
          "format": "heatmap",
          "interval": "",
          "legendFormat": "{{le}}",
          "refId": "A"
        }
      ],
      "title": "Token Utilization Heatmap",
      "type": "heatmap"
    },
    {
      "datasource": "Prometheus",
      "description": "Router tier selections across Tier 1 (Chat), Tier 2 (Reasoner), Tier 3 (Summary).",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 10
      },
      "id": 2,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "sum(increase(orch_tier_transition_total[5m])) by (tier)",
          "legendFormat": "{{tier}}",
          "refId": "A"
        }
      ],
      "title": "Tier Transition Histogram",
      "type": "barchart"
    },
    {
      "datasource": "Prometheus",
      "description": "Difference in tokens between hard truncation and semantic truncation.",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "orange",
                "value": 10
              },
              {
                "color": "red",
                "value": 25
              }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 10
      },
      "id": 3,
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "targets": [
        {
          "expr": "orch_semantic_truncation_delta_tokens",
          "legendFormat": "delta",
          "refId": "A"
        }
      ],
      "title": "Semantic Truncation Delta",
      "type": "gauge"
    }
  ],
  "schemaVersion": 39,
  "style": "dark",
